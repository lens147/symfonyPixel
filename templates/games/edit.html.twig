{% extends 'back.layout.html.twig' %}

{% block content %}

    <h1>Modifier le jeu "{{ entity.title }}"</h1>

{{ form_start(form) }}
{{ form_errors(form) }}
    <div class="card">
        <div class="card-body">
            {{ form_widget(form) }}
        </div>

        <div class="card-footer">
            <button type="submit" class="btn btn-primary">Enregistrer</button>
        </div>
    </div>
{{ form_end(form) }}
{% endblock %}
